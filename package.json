{
  "name": "marivel-testcafe",
  "version": "1.0.0",
  "description": "End-to-end tests for Mighty using Testcafe",
  "scripts": {
    "test": "testcafe chrome:headless testcafe/tests/*.js -e -q",
    "eslint": "eslint",
    "eslint-project": "eslint .",
    "eslint-check": "testcafe/bin/eslint-check.js",
    "prettier": "prettier --write",
    "prettier-project": "prettier --write 'testcafe/tests/*.js'"
  },
  "author": "Marivel Quiroz <marivel@able.co>",
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "prettier": "^1.17.0",
    "testcafe": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "testcafe/bin/eslint-check.js"
    }
  }
}
